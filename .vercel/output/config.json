{
	"version": 3,
	"routes": [
		{
			"src": "/_app/immutable/.+",
			"headers": {
				"cache-control": "public, immutable, max-age=31536000"
			}
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^/?(?:/__data.json)?$",
			"dest": "/fn-0"
		},
		{
			"src": "^/about/?(?:/__data.json)?$",
			"dest": "/fn-0"
		},
		{
			"src": "^/api/chat/?(?:/__data.json)?$",
			"dest": "/fn-1"
		},
		{
			"src": "^/api/posts/?(?:/__data.json)?$",
			"dest": "/fn-0"
		},
		{
			"src": "^/blog/?(?:/__data.json)?$",
			"dest": "/fn-0"
		},
		{
			"src": "^/blog/([^/]+?)/?(?:/__data.json)?$",
			"dest": "/fn-0"
		},
		{
			"src": "^/projects/?(?:/__data.json)?$",
			"dest": "/fn-0"
		},
		{
			"src": "^/tags/?(?:/__data.json)?$",
			"dest": "/fn-0"
		},
		{
			"src": "^/tags/([^/]+?)/?(?:/__data.json)?$",
			"dest": "/fn-0"
		},
		{
			"src": "/.*",
			"dest": "/fn"
		}
	],
	"overrides": {}
}